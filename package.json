{
  "name": "sesame3-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "dotenv -e .env -- prisma generate",
    "db:push": "dotenv -e .env -- prisma db push",
    "db:migrate": "dotenv -e .env -- prisma migrate dev",
    "db:studio": "dotenv -e .env -- prisma studio",
    "db:seed-all": "dotenv -e .env.local -- tsx prisma/seed-all.ts",
    "db:seed-programs": "dotenv -e .env.local -- tsx prisma/seed-programs.ts",
    "db:validate-programs": "tsx scripts/validate-programs-batch.ts",
    "db:seed-schools": "dotenv -e .env.local -- tsx prisma/seed-schools.ts",
    "db:seed-users": "dotenv -e .env.local -- tsx prisma/seed-users.ts",
    "db:cleanup": "dotenv -e .env.local -- tsx prisma/db-cleanup.ts",
    "db:clear-programs": "dotenv -e .env.local -- tsx prisma/db-cleanup.ts --programs",
    "db:clear-schools": "dotenv -e .env.local -- tsx prisma/db-cleanup.ts --schools",
    "db:clear-users": "dotenv -e .env.local -- tsx prisma/db-cleanup.ts --users",
    "db:clear-all": "dotenv -e .env.local -- tsx prisma/db-cleanup.ts --all",
    "db:fix-duplicates": "dotenv -e .env.local -- tsx prisma/db-cleanup.ts --duplicates",
    "db:clear-user": "dotenv -e .env.local -- tsx scripts/clear-user.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "ux-audit": "playwright test e2e/tests/ux-audit.spec.ts --project='Desktop Chrome'",
    "ux-audit:all": "playwright test e2e/tests/ux-audit.spec.ts",
    "ux-audit:mobile": "playwright test e2e/tests/ux-audit.spec.ts --project='Mobile Safari'",
    "test:uat": "playwright test e2e/tests/uat-student-journeys.spec.ts --project='Desktop Chrome'",
    "test:uat:all": "playwright test e2e/tests/uat-student-journeys.spec.ts",
    "test:report": "playwright show-report"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.56",
    "@ai-sdk/google": "^2.0.46",
    "@ai-sdk/groq": "^2.0.33",
    "@ai-sdk/openai": "^2.0.86",
    "@ai-sdk/react": "^2.0.115",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-email/components": "^1.0.2",
    "@react-email/render": "^2.0.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "ai": "^5.0.113",
    "clsx": "^2.1.1",
    "lucide-react": "^0.561.0",
    "next": "^15.5.9",
    "novel": "^1.0.2",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-markdown": "^10.1.0",
    "resend": "^6.6.0",
    "stripe": "^20.1.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "jsdom": "^27.3.0",
    "prisma": "^5.22.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.16"
  }
}
